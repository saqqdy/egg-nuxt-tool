{
	"name": "egg-nuxt-tool",
	"description": "egg plugin for nuxt render",
	"version": "1.0.1",
	"eggPlugin": {
		"name": "nuxtTool"
	},
	"scripts": {
		"test-local": "egg-bin test",
		"cov": "egg-bin cov",
		"lint": "eslint .",
		"ci": "egg-bin pkgfiles --check && npm run lint && npm run cov",
		"pkgfiles": "egg-bin pkgfiles",
		"autod": "autod",
		"test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
		"eslint": "eslint --fix --ext .vue,.js",
		"prettier": "npx prettier --write bin"
	},
	"dependencies": {
		"nuxt": "^2.14.7"
	},
	"devDependencies": {
		"autod": "^3.0.1",
		"autod-egg": "^1.1.0",
		"babel-eslint": "^10.1.0",
		"egg": "^2.16.0",
		"egg-bin": "^4.11.0",
		"egg-ci": "^1.11.0",
		"egg-mock": "^4.0.1",
		"eslint": "^7.13.0",
		"eslint-plugin-jsdoc": "^30.7.8",
		"jsdoc-to-markdown": "^6.0.1",
		"prettier": "^2.1.2",
		"uglify-js": "^3.10.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/egg-nuxt-tool.git"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"files": [
		"app.js",
		"agent.js",
		"config",
		"app"
	],
	"ci": {
		"version": "8, 10"
	},
	"keywords": [
		"egg-nuxt-tool",
		"nuxt",
		"render",
		"egg",
		"plugin",
		"eggPlugin",
		"egg-plugin"
	],
	"author": "saqqdy.com",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/saqqdy/egg-nuxt-tool/issues"
	},
	"homepage": "https://github.com/saqqdy/egg-nuxt-tool#readme"
}
